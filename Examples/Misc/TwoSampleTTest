input := (split("1 24 25 2 30 31 3 22 23 4 24 24 5 26 27 6 23 25 7 26 28 8 20 20 9 27 27 10 28 30", " ") | $0 as @number)

input := (input |? $0 > 10)
def noCup = (input |? $1 % 2 == 0)
def oneCup = (input |? $1 % 2 == 1)
def diff = oneCup - noCup

for (i: oneVar(diff)) {
    println i
}

